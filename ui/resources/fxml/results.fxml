<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.*?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<BorderPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
            stylesheets="@../style/results.css" xmlns="http://javafx.com/javafx/11.0.1"
            fx:controller="battleship.controllers.ResultsController"
            xmlns:fx="http://javafx.com/fxml/1">
    <top>
        <VBox prefHeight="200.0" prefWidth="100.0" styleClass="content-vbox" BorderPane.alignment="CENTER">
            <Label styleClass="header-label" text="Results"/>
            <Label fx:id="winnerNameLabel"/>
            <Label fx:id="myShotsLabel"/>
            <Label fx:id="partnerShotsLabel"/>
        </VBox>
    </top>
    <bottom>
        <Button mnemonicParsing="false" text="OK" BorderPane.alignment="CENTER" onAction="#onOkButtonAction"/>
    </bottom>
</BorderPane>
